import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.firefox.FirefoxDriver;

public class muruga {


		public static void main(String[] args) {
			// TODO Auto-generated method stub	System.setProperty("webdriver.gecko.driver","/usr/bin/geckodriver");
			// TODO Auto-generated method stub
			WebDriver driver = new FirefoxDriver();
		String a = "http://www.facebook.com";
			
	        //Launch the Online Store 
			driver.get(a);
			driver.findElement(By.name("email").sendkey("gopikingston.dy"));
			
	 
	        // Print a Log In message to the screen
	        System.out.println("Successfully opened the website www.Store.Demoqa.com");
			

	}

}
